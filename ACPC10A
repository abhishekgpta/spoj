#include<stdio.h>
int main(){
    int i;
     int arr[3];
    for(i=0;i<3;i++){
    		    scanf("%d",&arr[i]);
			}
        while(arr[0] !=0 || arr[1]!=0 || arr[2]!=0)
             {
			 if(arr[1]-arr[0] == arr[2]-arr[1])
	           printf("%s %d\n","AP",arr[2]+(arr[1]-arr[0]));
			else 
			   printf("%s %d\n","GP",(arr[2]*arr[2])/arr[1]);  
        	for(i=0;i<3;i++){
			    scanf("%d",&arr[i]);
			}
		}
	
    return 0;
}




